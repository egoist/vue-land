    "typeface-work-sans": "^0.0.22",
    "vue-github-badge": "^1.0.1",
    "vue-inline": "^1.0.1",
    "vue-media": "^1.0.0",
    "vue-no-ssr": "^0.1.2"
  },
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "husky": "^1.0.0-rc.14",
